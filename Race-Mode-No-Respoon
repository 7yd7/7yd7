

        while wait() do
            if _G.Auto == true then
        
                wait(0.2)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #1"]["Pass Detector"].CFrame
                            game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                                :ChangeState("Jumping")
                wait(0.2)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #2"]["Pass Detector"].CFrame
                            game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                                :ChangeState("Jumping")
                wait(0.2)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #3"]["Pass Detector"].CFrame
                            game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                                :ChangeState("Jumping")
                wait(0.2)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #4"]["Pass Detector"].CFrame
                            game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                                :ChangeState("Jumping")
                wait(0.2)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #5"]["Pass Detector"].CFrame
                            game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                                :ChangeState("Jumping")
                                wait(0.2)
                                                                                                                               
        wait(0.2)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Top_Section.Platform.Part.CFrame
        
        local New_CFrame = workspace.Top_Section.Hallway.RewardDoor.CFrame
        
        local ts = game:GetService('TweenService')
        local char = game.Players.LocalPlayer.Character
        
        local part = char.HumanoidRootPart
        local ti = TweenInfo.new(0.5, Enum.EasingStyle.Linear)
        local tp = {CFrame = New_CFrame}
        ts:Create(part, ti, tp):Play()
        wait(20)
        end
        end

    
