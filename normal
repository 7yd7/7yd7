_G.Auto 


while wait() do
    if _G.Auto == true then

        wait(0.3)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #1"].Checkpoint.Reach_Detector.CFrame
                    game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                        :ChangeState("Jumping")
        wait(0.3)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #2"].Checkpoint.Reach_Detector.CFrame
                    game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                        :ChangeState("Jumping")
        wait(0.3)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #3"].Checkpoint.Reach_Detector.CFrame
                    game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                        :ChangeState("Jumping")
        wait(0.3)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #4"].Checkpoint.Reach_Detector.CFrame
                    game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                        :ChangeState("Jumping")
        wait(0.3)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #5"].Checkpoint.Reach_Detector.CFrame
                    game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                        :ChangeState("Jumping")
                        wait(0.3)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #6"].Checkpoint.Reach_Detector.CFrame
                                    game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                                        :ChangeState("Jumping")
                                        wait(0.3)
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #7"].Checkpoint.Reach_Detector.CFrame
                                                    game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                                                        :ChangeState("Jumping")
                                                        wait(0.3)
                                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Tower["Floor #8"].Checkpoint.Reach_Detector.CFrame
                                                                    game:GetService "Players".LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'
                                                                        :ChangeState("Jumping")
                                                                                                                       
wait(0.6)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Top_Section.Platform.Part.CFrame

local New_CFrame = workspace.Top_Section.Hallway.RewardDoor.CFrame

local ts = game:GetService('TweenService')
local char = game.Players.LocalPlayer.Character

local part = char.HumanoidRootPart
local ti = TweenInfo.new(0.5, Enum.EasingStyle.Linear)
local tp = {CFrame = New_CFrame}
ts:Create(part, ti, tp):Play()
wait(20)
end
end

