local New_CFrame = workspace.Top_Section.Hallway.RewardDoor.CFrame

local ts = game:GetService('TweenService')
local char = game.Players.LocalPlayer.Character

local part = char.HumanoidRootPart
local ti = TweenInfo.new(0.5, Enum.EasingStyle.Linear)
local tp = {CFrame = New_CFrame}
ts:Create(part, ti, tp):Play()

wait(1.5)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-19.8579922, -10.9998655, 49.0710487, 0.00129685586, -1.0687764e-09, -0.999999166, 1.08730998e-08, 1, -1.05467646e-09, 0.999999166, -1.08717231e-08, 0.00129685586)

